@if (DemoCategory.Demos != null)
{
    @foreach (var category in DemoCategory.Demos)
    {
        <DemoCategoryItem Item="category" DemoSelected="Update" />
    }
}

@code {
    [Parameter]
    public EventCallback<string> DemoSelected { get; set; }

    private async Task Update(string code) => await DemoSelected.InvokeAsync(code);
}
