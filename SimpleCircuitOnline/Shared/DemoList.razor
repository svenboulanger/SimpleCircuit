@if (Demo.Demos != null)
{
    @foreach (var demo in Demo.Demos)
    {
        <a @onclick="x => Update(new(demo.Code, demo.Style))" class="bd-callout" role="button">
            <h5>@demo.Title</h5>
            <p>@demo.Description</p>
        </a>
    }
}

@code {
    [Parameter]
    public EventCallback<Netlist> DemoSelected { get; set; }

    private async Task Update(Netlist code) => await DemoSelected.InvokeAsync(code);
}
